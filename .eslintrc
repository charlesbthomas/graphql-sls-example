{
  "extends": "google",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": true,
    "codeFrame": false
  },

  "plugins": [],

  "rules": {
    "max-len": 0,
    "new-cap": 0,
    "default-case": 0,
    "prefer-const": 1,
    "require-jsdoc": 0,
    "no-unused-vars": 1,
    "comma-dangle": 0,
    "spaced-comment": 0,
    "padded-blocks": 0,
    "space-in-parens": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0,
    "import/extensions": 0,
    "indent": ["warn", 2],
    "no-multi-spaces": 0,
    "no-trailing-spaces": 1,
    "object-shorthand": 1,
    "class-methods-use-this": 0,
    "object-curly-spacing": 0,
    "arrow-parens": 0,
    "prefer-promise-reject-errors": 0
  }
}